<p id="notice"><%= notice %></p>

<h1>Listing Places</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Website</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Description</th>
      <th>Ratings</th>
      <th>City</th>
      <th>Country</th>
      <th>Fnt ratings</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <td><%= place.website %></td>
        <td><%= place.lat %></td>
        <td><%= place.long %></td>
        <td><%= place.description %></td>
        <td><%= place.ratings %></td>
        <td><%= place.city %></td>
        <td><%= place.country %></td>
        <td><%= place.fnt_ratings %></td>
        <td><%= place.start_time %></td>
        <td><%= place.end_time %></td>
        <td><%= link_to 'Show', place %></td>
        <td><%= link_to 'Edit', edit_place_path(place) %></td>
        <td><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place', new_place_path %>
